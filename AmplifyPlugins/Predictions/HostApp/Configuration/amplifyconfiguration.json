{
    "UserAgent": "aws-amplify-cli/2.0",
    "Version": "1.0",
    "auth": {
        "plugins": {
            "awsCognitoAuthPlugin": {
                "UserAgent": "aws-amplify/cli",
                "Version": "0.1.0",
                "IdentityManager": {
                    "Default": {}
                },
                "CredentialsProvider": {
                    "CognitoIdentity": {
                        "Default": {
                            "PoolId": "eu-west-2:421c84d0-6285-419b-a947-71c3e943a8ef",
                            "Region": "eu-west-2"
                        }
                    }
                },
                "CognitoUserPool": {
                    "Default": {
                        "PoolId": "eu-west-2_uhbZRCHrQ",
                        "AppClientId": "23nsmq2j27m8qevcemg6dqqksb",
                        "AppClientSecret": "1nugcu5hdjb07p0rcrrmc3g3um4sabq1dc0gvjs0rnqgb2c4h5g1",
                        "Region": "eu-west-2"
                    }
                },
                "Auth": {
                    "Default": {
                        "authenticationFlowType": "USER_SRP_AUTH"
                    }
                }
            }
        }
    },
    "predictions": {
        "plugins": {
            "awsPredictionsPlugin": {
                "defaultRegion": "eu-west-2",
                "interpret": {
                    "interpretText": {
                        "region": "eu-west-2",
                        "type": "ALL",
                        "defaultNetworkPolicy": "auto"
                    }
                },
                "identify": {
                    "identifyLabels": {
                        "region": "eu-west-2",
                        "type": "LABELS",
                        "defaultNetworkPolicy": "auto"
                    },
                    "identifyEntities": {
                        "maxEntities": "0",
                        "celebrityDetectionEnabled": "true",
                        "region": "eu-west-2",
                        "defaultNetworkPolicy": "auto"
                    },
                    "identifyText": {
                        "format": "ALL",
                        "region": "us-east-1",
                        "defaultNetworkPolicy": "auto"
                    }
                },
                "convert": {
                    "transcription": {
                        "language": "en-US",
                        "region": "eu-west-1",
                        "defaultNetworkPolicy": "auto"
                    }
                }
            }
        }
    }
}
